  m_Script: {fileID: 11500000, guid: a0b0bb453481480459f3d0640d64f23d, type: 3}
  m_Name: TutorialDialogue_1
  m_EditorClassIdentifier: 
  lines:
  - text: "\u2026\uC5B4\uC774? \uB4E4\uB824? \uC774\uBD10\u2014\uAC70\uAE30, \uB204\uC6CC
      \uC788\uB294 \uB108!"
    speaker: ???
    portrait: {fileID: 0}
  - text: "\uC774\uB807\uAC8C\uAE4C\uC9C0 \uC548 \uC77C\uC5B4\uB0A0 \uAC70\uC57C?
      \uC9C4\uC9DC \uC790\uB294\uAC70 \uB9DE\uC544?"
    speaker: ???
    portrait: {fileID: 0}
  - text: ...?
    speaker: Player
    portrait: {fileID: 0}
  - text: "\uC624~ \uB4DC\uB514\uC5B4 \uC77C\uC5B4\uB0AC\uB2E4! \uC640, \uC9C4\uC9DC
      \uC790\uACE0 \uC788\uC5C8\uB124?\n\uB098 \uC644\uC804 \uD070 \uBD09\uC778\uC774\uB77C\uB3C4
      \uD47C \uC904 \uC54C\uC558\uB2E4\uACE0~"
    speaker: ???
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uADF8 \uD45C\uC815 \uBB50\uC57C? \uC124\uB9C8 \uC544\uC9C1 \uAFC8\uC778
      \uC904 \uC544\uB294 \uAC70\uC57C?"
    speaker: ???
    portrait: {fileID: 21300000, guid: fbf52477b10f35c41a401cc541368979, type: 3}
  - text: "\uC9C4\uC9DC\uC57C, \uC9C0\uAE08 \uB124 \uC55E\uC5D0 \uB465\uB465 \uB5A0\uB2E4\uB2C8\uB294
      \uADF8 \uB9DD\uCE58. \uADF8 \uC548\uC5D0 \uB0B4\uAC00 \uB4E4\uC5B4\uC788\uB2E4\uACE0!
      \uAE5C\uC9DD \uB180\uB790\uC9C0?"
    speaker: ???
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uB098\uB294 \uD4E8\uB77C, \uB9DD\uCE58\uC5D0 \uAE43\uB4E0 \uC601\uD63C\uC774\uC57C!
      \uC5B4\uCA4C\uB2E4 \uC774\uB807\uAC8C \uB410\uB294\uC9C0\uB294 \uB098\uB3C4
      \uBAB0\uB77C! \uC544\uBB34 \uAE30\uC5B5\uB3C4 \uC5C6\uAC70\uB4E0! \uADF8\uB0E5
      \uB208 \uB728\uB2C8\uAE4C \uC774 \uC9D1\uC774\uB358\uB370?"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uADFC\uB370 \uC774\uC0C1\uD558\uC9C0 \uC54A\uC544? \uB2E4\uB978 \uC0AC\uB78C\uB4E4\uC740
      \uBABB \uB4E3\uB294 \uB0B4 \uBAA9\uC18C\uB9AC, \uB108\uB9CC \uB4E4\uB824. \uADF8\uB9AC\uACE0
      \uB0B4\uAC00 \uC774\uB807\uAC8C \uC2A4\uC2A4\uB85C \uB0A0\uC544\uAC04 \uAC83\uB3C4\u2026
      \uB108\uBFD0\uC774\uC57C."
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uADF8\uB7EC\uB2C8\uAE4C \uACB0\uB860! \uC9C0\uAE08\uBD80\uD130 \uB10C
      \uB0B4 \u2018\uD30C\uD2B8\uB108\u2019. \uC2EB\uC5B4\uB3C4 \uC5B4\uCA54 \uC218
      \uC5C6\uC5B4~ \uC120\uD0DD\uAD8C\uC740 \uC5C6\uC5B4~ "
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uB300\uC2E0 \uC88B\uC740 \uAC83 \uD558\uB098 \uC54C\uB824\uC904\uAC8C!
      \uC790, \uC55E\uC73C\uB85C \uB0B4\uAC00 \uD544\uC694\uD560 \uB550 \uADF8\uB0E5
      H\uD0A4 \uB20C\uB7EC\uBD10!"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uADF8\uB7FC \uB0B4\uAC00 \uC5B4\uB514\uC5D0 \uC788\uB4E0\u2014\uD4FD!
      \uD558\uACE0 \uB124 \uC190\uC5D0 \uB0A0\uC544\uAC08 \uAC70\uC57C!"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uADF8\uB9AC\uACE0 \uADF8 \uC548\uC5D0 \uC788\uB294 \uB098\uB294, \uB298
      \uC900\uBE44\uB3FC \uC788\uC73C\uB2C8\uAE4C~"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
  - text: "\uADF8\uB7FC \uBC14\uB85C \uD14C\uC2A4\uD2B8 \uD55C\uBC88 \uD574\uBCFC\uAE4C?
      H\uD0A4 \uC78A\uC740\uAC70 \uC544\uB2C8\uC9C0?"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 26032c306c7508041921c1715a1d21b9, type: 3}
