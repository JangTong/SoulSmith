  m_Script: {fileID: 11500000, guid: a0b0bb453481480459f3d0640d64f23d, type: 3}
  m_Name: TutorialDialogue_3
  m_EditorClassIdentifier: 
  lines:
  - text: "\uC88B\uC544, \uC774\uC81C \uC9C4\uC9DC \uB300\uC7A5\uC7A5\uC774 \uD749\uB0B4\uB77C\uB3C4
      \uC880 \uB0B4\uBCF4\uC790\uACE0! \uC8FC\uBB38\uC740 \uAD6C\uB9AC\uB85C \uB9CC\uB4E0
      \uAC80\uC774\uC5C8\uB098?"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 7d07a200e4c57884daf87670666464ec, type: 3}
  - text: "\uC5EC\uAE30\uAC00 \uBC14\uB85C \uC6A9\uAD11\uB85C! \uB728\uAC81\uACE0,
      \uC704\uD5D8\uD558\uACE0, \uC644\uC804 \uBA4B\uC9C4 \uACF3\uC774\uC9C0!"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: acb8c4123869c564b980ad89a80f473e, type: 3}
  - text: "\uBA3C\uC800 \uC8FC\uBCC0\uC5D0\uC11C \uAE08\uC18D\uC774\uB791 \uC11D\uD0C4\uC744
      \uCC3E\uC544. \uB0B4\uAC00 \uD45C\uC2DC \uD574\uB454 \uACF3\uB4E4 \uC911 \uD558\uB098\uC5D0
      \uC788\uC744\uAC70\uC57C!\n\uC544\uC774\uD15C\uC740 \uC624\uB978\uC190\uC73C\uB85C
      \uC8FC\uC73C\uB824\uBA74 [\uC88C\uD074\uB9AD], \uC67C\uC190\uC5D0 \uC8FC\uC73C\uB824\uBA74
      [Q]\uB97C \uB204\uB974\uBA74 \uB3FC! \uC5B4\uB835\uC9C0 \uC54A\uC9C0?"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 7d07a200e4c57884daf87670666464ec, type: 3}
  - text: "\uADF8\uB2E4\uC74C\uC5D4, [\uC6A9\uAD11\uB85C]\uC5D0 \uAE08\uC18D\uC774\uB791
      \uC5F0\uB8CC\uB97C 1:1 \uBE44\uC728\uB85C \uB123\uB294 \uAC70\uC57C.\n\uD558\uB098
      \uB123\uC5C8\uC73C\uBA74, \uB2E4\uB978 \uCABD\uB3C4 \uD558\uB098. \uADE0\uD615\uC774
      \uC911\uC694\uD558\uAC70\uB4E0~"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 7d07a200e4c57884daf87670666464ec, type: 3}
  - text: "\uB2E4 \uB123\uC5C8\uC73C\uBA74, \uC800 \uC606\uC5D0 \uC788\uB294 [\uD480\uBB34]\uB97C
      \uD55C\uBC88 \uC2DC\uC6D0\uD558\uAC8C \uB20C\uB7EC\uC918!\n\uADF8\uB7EC\uBA74\u2014\uBD80\uC6C5!
      \uD558\uACE0 \uBD88\uAF43\uC774 \uC62C\uB77C\uC624\uBA74\uC11C \uC791\uC5C5
      \uC2DC\uC791!"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 7d07a200e4c57884daf87670666464ec, type: 3}
  - text: "\u2026\uBB3C\uB860, \uC2E4\uD328\uD558\uBA74 \uC5F0\uAE30\uB9CC \uD3F4\uD3F4
      \uB0A0\uC9C0\uB3C4 \uBAA8\uB974\uB2C8\uAE4C \uC870\uC2EC\uD574~"
    speaker: "\uD4E8\uB77C"
    portrait: {fileID: 21300000, guid: 7d07a200e4c57884daf87670666464ec, type: 3}
